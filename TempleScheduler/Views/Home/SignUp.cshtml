

@{
    string[] times = {"Monday 8:00am - 9:00am","Monday 9:00am - 10:00am","Monday 10:00am - 11:00am","Monday 11:00am - 12:00pm",
        "Monday 12:00pm - 1:00pm","Monday 1:00pm - 2:00pm","Monday 2:00pm - 3:00pm"," Monday 3:00pm - 4:00pm"," Monday 4:00pm - 5:00pm",
        " Monday5:00pm - 6:00pm","Monday 6:00pm - 7:00pm","Monday 7:00pm - 8:00pm","Monday 8:00pm - 9:00pm",
        "Tuesday 8:00am - 9:00am","Tuesday 9:00am - 10:00am","Tuesday 10:00am - 11:00am","Tuesday 11:00am - 12:00pm",
        "Tuesday 12:00pm - 1:00pm","Tuesday 1:00pm - 2:00pm","Tuesday 2:00pm - 3:00pm"," Tuesday 3:00pm - 4:00pm"," Tuesday 4:00pm - 5:00pm",
        " Tuesday 5:00pm - 6:00pm","Tuesday 6:00pm - 7:00pm","Tuesday 7:00pm - 8:00pm","Tuesday 8:00pm - 9:00pm",
        "Wednesday 8:00am - 9:00am","Wednesday 9:00am - 10:00am","Wednesday 10:00am - 11:00am","Wednesday 11:00am - 12:00pm",
        "Wednesday 12:00pm - 1:00pm","Wednesday 1:00pm - 2:00pm","Wednesday 2:00pm - 3:00pm"," Wednesday 3:00pm - 4:00pm"," Wednesday 4:00pm - 5:00pm",
        "Wednesday 5:00pm - 6:00pm","Wednesday 6:00pm - 7:00pm","Wednesday 7:00pm - 8:00pm","Wednesday 8:00pm - 9:00pm",
        "Thursday 8:00am - 9:00am","Thursday 9:00am - 10:00am","Thursday 10:00am - 11:00am","Thursday 11:00am - 12:00pm",
        "Thursday 12:00pm - 1:00pm","Thursday 1:00pm - 2:00pm","Thursday 2:00pm - 3:00pm"," Thursday 3:00pm - 4:00pm"," Thursday 4:00pm - 5:00pm",
        "Thursday 5:00pm - 6:00pm","Thursday 6:00pm - 7:00pm","Thursday 7:00pm - 8:00pm","Thursday 8:00pm - 9:00pm",
        "Friday 8:00am - 9:00am","Friday 9:00am - 10:00am","Friday 10:00am - 11:00am","Friday 11:00am - 12:00pm",
        "Friday 12:00pm - 1:00pm","Friday 1:00pm - 2:00pm","Friday 2:00pm - 3:00pm"," Friday 3:00pm - 4:00pm"," Friday 4:00pm - 5:00pm",
        "Friday 5:00pm - 6:00pm","Friday 6:00pm - 7:00pm","Friday 7:00pm - 8:00pm","Friday 8:00pm - 9:00pm",
        "Saturday 8:00am - 9:00am","Saturday 9:00am - 10:00am","Saturday 10:00am - 11:00am","Saturday 11:00am - 12:00pm",
        "Saturday 12:00pm - 1:00pm","Saturday 1:00pm - 2:00pm","Saturday 2:00pm - 3:00pm"," Saturday 3:00pm - 4:00pm"," Saturday 4:00pm - 5:00pm",
        "Saturday 5:00pm - 6:00pm","Saturday 6:00pm - 7:00pm","Saturday 7:00pm - 8:00pm","Saturday 8:00pm - 9:00pm",
    };

    foreach(var x in ViewBag.TimesList)
    {
        for (var iCount = 0; iCount < times.Length; iCount++)
        {
            if (x.Timeslots == times[iCount])
            {
                List<string> timesList = new List<string>(times);
                timesList.RemoveAt(iCount);
                times = timesList.ToArray();
                iCount--;
            }
        }
    }
}

@model TempleScheduler.Models.AvailableTime

<body>
    <br /><br /><br /><br /><br /><br />
    <form asp-action="SignUp" method="post" class="center">
        <div class="text-center">
            <label name="Timeslots">Choose an available time slot(Closed on Sunday): </label>
            <select name="Timeslots" id="Timeslots" asp-for="Timeslots" required>
                <option value="none" style="display:none" disabled="disabled" selected="selected">Select One:</option>
                @for (var iCount = 0; iCount < times.Length; iCount++)
                {
                    <option value="@times[iCount]">@times[iCount]</option>
                }
            </select>
            <br />
            <div class="text-center">
                <button type="submit">Continue</button>
            </div>
            <div asp-validation-summary="All"></div>
            <img src="~/img/Come.png" alt="Come Follow Me" height="500" width="400" />
        </div>
    </form>
</body>